import java.lang.reflect.Array;

public class linearSearch {

	public static void main(String[] args) {
		int[] arr = {1, 2, 3, 4, 5};
		int target = 5;
		int res = 0;
		
		for(int i =0; i<arr.length; i++) {
			if(arr[i] == target) {
				res = i;
			}else {
				res = 0;
			}
			
		}
		System.out.println(res);
		
	}
}
