import java.awt.*;

public class AWT extends Frame{
	
	AWT(){
		Button btn = new Button("Submit");
		Button btn1 = new Button("Clear");
		
		add(btn);
		add(btn1);
		
		setLayout(new FlowLayout());
		setVisible(true);
		setSize(100, 100);
	}

	public static void main(String[] args) {
		new AWT();

	}

}
